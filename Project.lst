				    mov     edx, promptAddr
				    call    WriteString
				
				    mov     edx, userInputAddr
				    mov     ecx, MAX_LENGTH
				    call    ReadString    mov     edi, byteCountAddr
				    mov     [edi], eax
				    pop     edi    pop     eax    pop     ecx    pop     edxendm
				
				mDisplayString  macro stringAddr
				    push    edx    mov     edx, stringAddr
				    call    WriteString    pop     edxendm
				
